{
    "version": 2,
    "builds": [
        {
        "src": "/src/*.js",
        "use": "@now/node"
        },
        {
        "src": "/src/routes/*.js",
        "use": "@now/node"
        }
    ],
    "routes":[
        {
            "src": "/src/(.*)",
            "dest": "/src/$1"
        }
    ]
}